{
    "framework": "vite",
    "buildCommand": "vite build",
    "outputDirectory": "dist",
    "functions": {
        "api/extract.js": {
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}